  "scripts": {
    "build": "webpack --progress --colors --watch --config webpack/webpack.dev.config.js",
    "dev": "webpack-dev-server --port 8001 --progress --colors --display-error-details --inline --hot --content-base root/build --config webpack/webpack.dev.config.js",
    "clean": "rm -rf dist/staging/assets && rm -rf dist/production/assets",
    "staging": "npm run clean && NODE_ENV=production webpack -p --progress --display-error-details --config webpack/webpack.staging.config.js",
    "production": "npm run clean && NODE_ENV=production webpack -p --progress --display-error-details --config webpack/webpack.production.config.js"
  },
